  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/frida-gum": "^17.3.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^17.0.10",
    "chai": "^4.3.7",
    "mocha": "^10.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.4"
